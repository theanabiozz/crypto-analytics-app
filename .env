# Порт для сервера Express (по умолчанию 3001)
PORT=3001

# URI для подключения к MongoDB
# Замените на ваш собственный URI для MongoDB Atlas или локальной базы данных
MONGODB_URI=mongodb://localhost:27017/crypto_patterns

# Секретный ключ для JWT
JWT_SECRET=your_jwt_secret_key_change_this_in_production

# Режим работы (development или production)
NODE_ENV=development